<h1>Pics</h1>
<div class="row">
  
  {{#each stories}}
    <div class="col s12 m4">
     
   
     {{!-- src="uploads/{{filename}}" --}}
      <div class="card" >
        <div class="card-image">
          
          {{!-- <img   alt="{{filename}}" height="200" width="200" src="https://frozen-taiga-62509.herokuapp.com/uploads/{{filename}}" > --}}
          <img   alt="{{filename}}" height="200" width="200" src="{{process.env.PWD}}/uploads/{{filename}}" > 
        
        </div>
        <div class="card-content center-align">
         
         
          <div class="chip">
            <img src="{{user.image}}">
            <a href="/pics/user/{{user.id}}">{{user.firstName}} {{user.lastName}}</a>

          </div>
        </div>
        <div class="card-action center-align" style="width: 20rem">
           <a class="btn grey" href="/pics/show/{{id}}">Show More</a> 
                     
                   

        </div>
      </div>
    </div>
  {{else}}
    <p>No Pics found</p>
  {{/each}}
</div>
